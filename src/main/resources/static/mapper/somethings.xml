<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.cigarettesmakeshower.Dao.SomeDao">
    <select id="getSomes" resultType="com.example.cigarettesmakeshower.Model.SomeModel">
		select * from SomeModel
	</select><insert id="insertSome" parameterType="com.example.cigarettesmakeshower.Model.SomeModel">
		insert into SomeModel
		(
			title,price
		)
		values(
			#{title},#{price}
		)
	</insert>
</mapper>
